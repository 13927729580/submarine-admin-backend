{
    "db": {
        "host": "192.168.39.36",
        "user": "root",
        "password": "root",
        "database": "submarine"
    },
    "mixin": {
        "packagePath": "org/javahub/submarine",
        "package": "org.javahub.submarine",
        "module": "system",
        "entity": "",
        "Entity": "",
        "entityZh": ""
    },
    "fields": [
        {
            "title": "下拉选择示范",
            "name": "enable",
            "default": "enable",
            "type": "select",
            "options": [
                { "label": "启用", "value": "enable" },
                { "label": "冻结", "value": "disable" }
            ]
        },
        {
            "title": "输入框示范",
            "name": "address",
            "default": "",
            "type": "input"
        }
    ],
    "input": {
        "dir": "./templates/"
    },
    "output": [
        {
            "template": "controller.ejs",
            "path": "../java/<%= packagePath %>/modules/<%= module %>/controller/<%= Entity %>Controller.java"
        },
        {
            "template": "service.ejs",
            "path": "../java/<%= packagePath %>/modules/<%= module %>/service/<%= Entity %>Service.java"
        },
        {
            "template": "mapper.ejs",
            "path": "../java/<%= packagePath %>/modules/<%= module %>/mapper/<%= Entity %>Mapper.java"
        },
        {
            "template": "entity.ejs",
            "path": "../java/<%= packagePath %>/modules/<%= module %>/entity/<%= Entity %>.java"
        },
        {
            "template": "dto.ejs",
            "path": "../java/<%= packagePath %>/modules/<%= module %>/dto/<%= Entity %>Dto.java"
        },
        {
            "template": "xml.ejs",
            "path": "../java/<%= packagePath %>/modules/<%= module %>/xml/<%= Entity %>Mapper.xml"
        }
    ]
}
